
@{
    ViewBag.Title = "Select_2s";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
    SelectList list = ViewBag.MAQH;
}

    <div style="width: 50%; margin: auto;">
        <h2>
            Chọn quận / huyện
        </h2>
        <form name="q2" class="row container-ssa" action="/User/Select_3s" method="post">
            <select name="id" class="form-control col-lg-8">
                @foreach (var item in list)
                {
                    <option value="@item.Value">@item.Text</option>
                }
            </select>
            <div class="col-lg-4"><button id="reg" class="btn btn-success">CHỌN</button></div>
        </form>
    </div>